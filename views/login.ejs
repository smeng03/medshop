<!--either log in to your account or create an account through the link-->
<div class="container">
<h1>Log In</h1>
<br/>
<form action="/message2" method = "POST">
  <div class="form-group">
    <label for="username">Username*:</label>
    <input type="text" class="form-control" placeholder="Username" name="username" id="username" required="true">
  </div>
  <br/>
  <div class="form-group">
    <label for="password">Password*:</label>
    <input type="password" class="form-control" placeholder="Password" name="pwd" id="pwd" required="true">
  </div>
  <br/>
  <button type="submit" class="btn btn-default" id = "submit">Log In</button>
  <br/><br/>
  <a href="/account" class="bigfont">New user? Click here to create an account</a>
</form>
</div>
<br/><br/>

